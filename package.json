{
  "name": "useless-calculator",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "app:dir": "electron-builder --dir",
    "app:dist": "electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "electron": "^35.1.5",
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "Calculator",
    "productName": "calculator",
    "compression": "maximum",
    "files": [
      "index.html",
      "index.css",
      "main.js",
      "preload.js",
      "renderer.js",
      "icon.png",
      "node_modules/**/*"
    ],
    "extraFiles": [
      "build/**/*"
    ],
    "asar": true,
    "win": {
      "icon": "build/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "expr-eval": "^2.0.2"
  }
}
